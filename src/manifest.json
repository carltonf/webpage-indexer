{
  "name": "HTML Outline Indexer",
  "short_name": "html-ol-indexer",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "An interactive tool to generate an outline/index for a web page",
  "icons": {
  },
  "background": {
    "scripts": [
      "scripts/background/init.js",
      "scripts/background/inject.js",
      "scripts/background/main.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [],
  "browser_action": {
    "default_title": "ol-indexer"
  }
}
